@using WhiteBears.Models
@model IEnumerable<ProjectPageViewModel>


<tbody id="projectTaskTableTbody" class="projectTableTbody">
    @foreach (var task in Model)
    {
        <tr id="@task.Task.TaskId" class="taskID">
            <td id="isCompleted">
                @Html.CheckBox("task" + task.Task.TaskId, @task.Task.IsCompleted)
            </td>
            <td class="taskTB">
                @task.Task.Title
            </td>
            <td class="taskTB">
                @task.Task.DueDate.ToShortDateString()
            </td>

        </tr>
    }
</tbody>

