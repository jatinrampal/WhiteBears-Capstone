@using WhiteBears.Models
@model IEnumerable<ProjectPageViewModel>


<tbody id="projectTaskTableTbody" class="projectTableTbody">
    @foreach (var task in Model)
    {
        <tr id="@task.Task.TaskId">
            <td>
                @Html.CheckBox("task", @task.Task.IsCompleted)
            </td>
            <td>
                @task.Task.Title
            </td>
            <td>
                @task.Task.DueDate
            </td>
        </tr>
    }
</tbody>

